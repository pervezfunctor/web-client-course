{
  "name": "vite-starter",
  "version": "0.0.1",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "lint:dev": "pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm"
  },
  "dependencies": {
    "@chakra-ui/react": "2.4.3",
    "@chakra-ui/styled-system": "2.4.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@srtp/core": "workspace:*",
    "@srtp/todo": "workspace:*",
    "framer-motion": "8.0.0",
    "immer": "9.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.4.1",
    "use-immer": "0.8.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "13.4.0",
    "@types/node": "^18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.30.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.6.0",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "vitest": "0.26.0"
  }
}
