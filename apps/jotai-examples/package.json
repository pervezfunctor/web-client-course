{
  "name": "jotai-examples",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "coverage": "vitest run --coverage",
    "db": "json-server ./db.js",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "dev": "concurrently pnpm:web:dev pnpm:db",
    "lint:dev": "pnpm lint --quiet --fix",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "serve": "vite preview",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "web:dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false"
  },
  "dependencies": {
    "@chakra-ui/react": "2.4.3",
    "@chakra-ui/styled-system": "2.4.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "framer-motion": "8.0.0",
    "immer": "9.0.16",
    "jotai": "1.12.0",
    "jotai-immer": "0.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.4.1",
    "use-immer": "0.8.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/todo": "workspace:*",
    "@testing-library/react": "13.4.0",
    "@types/node": "^18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "concurrently": "^7.6.0",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "45.0.2",
    "npm-check-updates": "16.6.0",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "vitest": "0.26.0"
  }
}
