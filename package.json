{
  "name": "vite-starter",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "lint:fix": "pnpm lint --quiet --fix",
    "types:ci": "tsc --noEmit -p .",
    "types": "tsc --noEmit -p . --watch",
    "ci": "pnpm types:ci && pnpm lint:fix && vitest run",
    "test": "vitest dev",
    "json": "pnpm json-server --watch ./db.json",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/core": "7.18.10",
    "@chakra-ui/react": "2.2.8",
    "@chakra-ui/styled-system": "^2.2.7",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@reduxjs/toolkit": "1.8.4",
    "@tanstack/react-query": "4.2.1",
    "@xstate/inspect": "0.7.0",
    "@xstate/react": "3.0.1",
    "framer-motion": "7.2.0",
    "http-status-codes": "2.2.0",
    "immer": "9.0.15",
    "jotai": "1.8.0",
    "observable-hooks": "4.2.0",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "recoil": "0.7.5",
    "redaxios": "0.5.1",
    "redux": "4.2.0",
    "rxjs": "7.5.6",
    "swr": "1.3.0",
    "tiny-invariant": "1.2.0",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.2.0",
    "tslib": "2.4.0",
    "use-immer": "0.7.0",
    "valtio": "1.6.4",
    "xstate": "4.33.2",
    "zod": "3.18.0"
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "43.0.2",
    "json-server": "0.17.0",
    "npm-check-updates": "16.0.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.22.1",
    "ws": "8.8.1"
  }
}
